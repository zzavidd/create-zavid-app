{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": false,
    "outDir": "./dist",
    "skipLibCheck": true,
    "strict": true,
    "target": "es6",
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "**/config/src/**"]
}
