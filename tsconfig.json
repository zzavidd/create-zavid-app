{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext"],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "target": "es5",
    "isolatedModules": false,
    "module": "commonjs",
    "noEmit": false,
    "outDir": "./dist"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "**/config/src/**"]
}
